{
  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ]
}
