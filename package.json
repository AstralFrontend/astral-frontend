{
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/*/*",
    "scripts"
  ],
  "scripts": {
    "jest": "jest",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "yaspeller": "yaspeller . --only-errors",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests",
      "yaspeller",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@sentry/browser": "^6.2.3",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "acorn": "^7.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "chromatic": "^5.7.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "css-loader": "3.4.2",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-github-actions-reporter": "^1.0.3",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "1.1.3",
    "webpack": "^4.41.6",
    "yaspeller": "^7.0.0"
  },
  "dependencies": {}
}
